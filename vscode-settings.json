{
  "files.hotExit": "onExitAndWindowClose",
  "editor.scrollbar.verticalScrollbarSize": 15,
  "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.wordWrap": "on",
  "editor.insertSpaces": false,
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "all",
  "editor.minimap.enabled": false,
  "editor.codeLens": false,
  "editor.colorDecorators": false,
  "editor.glyphMargin": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.scrollBeyondLastLine": false,
  "editor.tabCompletion": "on",
  "editor.overviewRulerBorder": false,
  "editor.folding": true,
  "editor.lineNumbers": "on",
  "editor.autoSurround": "never",
  "editor.lightbulb.enabled": "off",
  "editor.stickyScroll.enabled": true,
  "editor.autoClosingBrackets": "never",
  "editor.autoIndent": "keep",
  "editor.inlineSuggest.enabled": true,
  "editor.rulers": [],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "visible",
  "editor.accessibilitySupport": "off",
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "window.dialogStyle": "custom",
  "window.restoreWindows": "folders",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 1,
  "window.customTitleBarVisibility": "windowed",
  "window.commandCenter": false,
  "workbench.startupEditor": "none",
  "workbench.layoutControl.type": "toggles",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.editorAssociations": {
    "*.plist": "default",
    "*.pdf": "default"
  },
  "workbench.colorCustomizations": {
    "editorOverviewRuler.border": "#0000",
    "editorOverviewRuler.findMatchForeground": "#0000",
    "editorOverviewRuler.rangeHighlightForeground": "#0000",
    "editorOverviewRuler.selectionHighlightForeground": "#0000",
    "editorOverviewRuler.wordHighlightForeground": "#0000",
    "editorOverviewRuler.wordHighlightStrongForeground": "#0000",
    "editorOverviewRuler.modifiedForeground": "#0000",
    "editorOverviewRuler.addedForeground": "#0000",
    "editorOverviewRuler.deletedForeground": "#0000",
    "editorOverviewRuler.errorForeground": "#0000",
    "editorOverviewRuler.warningForeground": "#0000",
    "editorOverviewRuler.infoForeground": "#0000",
    "editorOverviewRuler.bracketMatchForeground": "#0000"
  },
  "files.exclude": {
    ".DS_Store": true,
    ".git/": true,
    ".vscode/": true,
    "*.class": true,
    "*.code-workspace": true,
    "**/.DS_Store": false,
    "**/.git": false,
    "**/.hg": false,
    "**/.svn": false,
    "**/CVS": false,
    "**/Thumbs.db": false,
    "**/__pycache__/": true,
    "*/.DS_Store": true,
    "*/.editorconfig": true,
    "*/.git/": true,
    "*/.gitattributes": true,
    "*/*.class": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.excludeGitIgnore": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.external.osxExec": "kitty.app",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  "problems.decorations.enabled": false,
  "outline.problems.enabled": false,
  "outline.problems.colors": false,
  "outline.problems.badges": false,
  "scm.diffDecorations": "none",
  "breadcrumbs.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "extensions.ignoreRecommendations": true,
  "debug.onTaskErrors": "showErrors",
  "C_Cpp.default.compilerPath": "clang",
  "C_Cpp.errorSquiggles": "disabled",
  "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: Google, IndentWidth: 4}",
  "cmake.configureOnOpen": true,
  "cmake.showOptionsMovedNotification": false,
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[python]": {
    "editor.wordBasedSuggestions": "off"
  },
  "[java]": {
    "editor.defaultFormatter": "Oracle.oracle-java"
  },
  "python.defaultInterpreterPath": "/Users/basil/Developer/env",
  "java.inlayHints.parameterNames.enabled": "none",
  "java.project.importHint": false,
  "java.server.launchMode": "LightWeight",
  "vim.cursorStylePerMode.normal": "underline",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.visual": "block",
  "github.copilot.editor.enableAutoCompletions": true,
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "editor.formatOnSave": true
}
